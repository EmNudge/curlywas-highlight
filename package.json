{
  "name": "curlywas-highlight",
  "displayName": "curlywas-highlight",
  "description": "Highlight curlywas files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "curlywas",
        "aliases": [
          "CurlyWas",
          "curlywas"
        ],
        "extensions": [
          "cwa"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "curlywas",
        "scopeName": "source.cwa",
        "path": "./syntaxes/curlywas.tmLanguage.json"
      }
    ]
  }
}
